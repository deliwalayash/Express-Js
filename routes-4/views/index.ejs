<link rel="stylesheet" href="	https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css">

<div class="mt-4 mb-4">
  <div class="text-center">
    <a href="/form"><button >ADD USER</button></a>
  </div>
    <ul class="list-unstyled container">
    <% users.forEach(element => { %>
     <div class="mb-3">
     <li><a href="/showdata/<%= element.id  %>"><%= element.id  %>.<%= element.first_name  %></a>
      <a href="/delete/<%= element.id %>"><button>Delete</button></a>
      <a href="/edit/<%= element.id %>"><button>Edit</button></a>
    </li>
     </div>
    <% }) %>
</ul >
<br>
</div>